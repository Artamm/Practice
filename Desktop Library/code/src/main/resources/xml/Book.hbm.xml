<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "Data.Book" table = "books">
        <id name = "number" type = "int" column = "id">
            <generator class="native"/>
        </id>
        <property name="title" column="Title" type="java.lang.String" />
        <property name="year" column="Year" type="java.lang.Integer" />
        <property name="genre" column="Genre" type="java.lang.String" />
        <property name="authorsString" column="Author" type="java.lang.String" />
        <property name="date" column="Date" type="java.lang.String"/>



    </class>
</hibernate-mapping>